
<img src="<%= pokemon.sprites.front_default %>" alt="">

<ul>
    <li>
        <h1>Name: <%= pokemon.name %></h1>
        <h1>Hieght: <%= pokemon.height %></h1>  
        <h1>Weight: <%= pokemon.weight %></h1> 
        <h1>Type: <%= pokemon.types[0].type.name %></h1>
    </li>
</ul>
</br>
<ul><h1>Abilities: </h1>
    <% pokemon.abilities.forEach(ability => { %>
        <h1><li><%= ability.ability.name %></li></h1>
    <% }) %>
</ul>
