<h2>poke info</h2>
<h3><%=pokemon.name%></h3>
<img src="<%=officialArt%>">
<h3>Type(s):</h3>
<ul>
    <%pokemon.types.forEach(function(type){%>
        <li>
            <%=type.type.name%>
        </li>
    <%})%>
</ul>
<h3>Abilities:</h3>
<ul>
    <%pokemon.abilities.forEach(function(ability){%>
        <li>
            <%=ability.ability.name%>
        </li>
    <%})%>
</ul>