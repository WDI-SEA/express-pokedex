<div class="container">
  <h1 class="text-center">Favorites</h1>
  <% pokemon.forEach(function(pokemon) { %>
    <div class="well">
      <h2><%= pokemon.name %></h2>
      <form method="GET" action="/pokemon/<%= pokemon.id %>">
        <button class="btn btn-primary" type="submit">Show Abilites</button>
      </form>
    </div>

  <% }); %>
</div>
