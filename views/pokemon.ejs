<h1 class="text-center">Your Favorites</h1>
<% if(results.length <= 0) { %>
	<h2>Nothing in the database yet</h2>
<% } else { %>
	<% results.forEach(function(item){ %>
		<h3>
			<a href="/pokemon/<%= item.id %>"><%= item.name %></a>
		</h3>
		<a href="/pokemon/<%= item.id %>" class="btn btn-danger delete-link">Delete</a>
	<% }); %>
<% } %>
