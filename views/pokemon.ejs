<div class="container">
   <h1>Favorite Pokemon</h1>
   <p>
     <a href="/">Back to all Pokemon</a>
   </p>

   <% pokemon.forEach(function(pokemon) { %>
     <div class="pokeDiv" id="">
      <a href="/pokemon/<%= pokemon.name %>"><h2><%= pokemon.name %></h2></a>
       <button class="delete-link">Remove from Favorites</button>
     </div>
   <% }); %>

 </div>
