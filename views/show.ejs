<!-- attributes page -->
<div class="container">
    <h1 class="text-center">Pokemon Details Page</h1>
    <p class="text-center">
        <a href="/">Back to Homepage</a>
        <a href="/pokemon">Back to Favorite Pokemon</a>
    </p>

    <% pokemon.types.forEach(function(pokemon) {%>
        <div class ='well'>
            <h2>Type: <%= pokemon.type.name %></h2>
        </div>
    <% }) %>

    <% pokemon.abilities.forEach(function(pokemon) {%>
        <div class ='well'>
            <h2>Ability: <%= pokemon.ability.name %></h2>
        </div>
    <% }) %>

    <div class ='well'>
        <h2>Base experience: <%= pokemon.base_experience %></h2>
    </div>
    
    <div class ='well'>
        <h2>Weight: <%= pokemon.weight %></h2>
    </div>
    

 </div>