<div class="container">
    <h1 class="text-center">Your Favorite Pokemon</h1>
    <p class="text-center">
     
    </p>
  
    <% pokemon.forEach(function(pokemon) { %>
      <div class="well">
        <h2><%= pokemon.name %></h2>
        <a href="/pokemon/<%= pokemon.name %>">Click for more info</a>
      </div>
    <% }); %>
  </div>