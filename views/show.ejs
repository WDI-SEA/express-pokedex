<div class="container">
    <div class='child'>
        <h2><%= pokedata.name.toUpperCase()%> | Exp: <%= pokedata.base_experience%></h2>
        <img class="sprites" src="<%= pokedata.sprites.front_default %>" alt="Pokemon image">
    
        
        <h4>Weight: <%= pokedata.weight %> hectograms | Height: <%= pokedata.height %> decimetres </h4>
        <h3>Type: <%= pokedata.types[0].type.name %></h3>
        <h3>Power: <%= pokedata.abilities[1].ability.name %></h3>

    </div>

</div>

