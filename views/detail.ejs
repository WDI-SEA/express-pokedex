<div class="container">
    <span><img src="<%= pokemon.sprites.front_default %>" alt=""><h1><%= pokemon.name %></h1></span>

    <img src="<%= pokemon.sprites.other["official-artwork"].front_default %>" alt="official artwork">
    
    <p>height: <%= pokemon.height %></p>
    
    <p>weight: <%= pokemon.weight %></p>

    <ul>Type(s): <% pokemon.types.forEach(function(type) { %>
        <li>
            <%= type.type.name %>
        </li>   
        <% }) %>
    </ul>

    <ul>Ability(ies): <% pokemon.abilities.forEach(function(ability) { %>
        <li>
            <%= ability.ability.name %>
        </li>   
        <% }) %>
    </ul>
           
</div>