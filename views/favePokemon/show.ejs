<h1><%= name %></h1>

    <img src="<%= image %> " alt="picture of pokemon">
    
<h4>List of abilites</h4>
 <ul>
     <% abilities.forEach(ability => { %>
         <li><%=ability.ability.name%></li>
    <% }) %>
 </ul> 

 <h4>List of Moves</h4>
 <ul>
     <% moves.forEach(move => { %>
         <li>
            <%=move.move.name%>
        </li>
     <% }) %>
 </ul>

 <h4>Stats</h4>
 <ul>
     <% stats.forEach(stat =>{ %>
         <li>
            <%= stat.stat.name%>   
        </li>
    <% }) %>
 </ul>