<h1>All Your Favorite Pokemon</h1>
<%results.forEach(result=> {%>
    <h3>
        <a href="/pokemon/<%= result.name %>">
            <%= result.name %>
        </a>
    </h3>
    <form action="/faves/<%= result.id %>?_method=DELETE" method="post">
        <button type="submit" class="btn btn-danger">Remove From Faves</button>
    </form>
    <%})%>