<div class="container">
  <h1 class="text-center">The Original 151 Pokemon</h1>
  

  <% pokemon.forEach(function(pokemon) { %>
    <div class="well">
      <h2><%= pokemon.name %></h2>
      <form method="POST" action="/pokemon">
        <input type="hidden" name="name" value="<%= pokemon.name %>">
        <input class="btn btn-primary" type="submit" value="Add to Favorite Pokemon">
      </form>
    </div>
  <% }); %>
</div>
