<div>
  <a href="https://fontmeme.com/pokemon-font/"><img src="https://fontmeme.com/permalink/200810/ab878fa388275e681f4d3c92e52b2449.png" alt="pokemon-font" border="0"></a>

  <% pokemon.forEach(function(pokemon) { %>
    <div class="well">
      <h2><%= pokemon.name %></h2>
      <form method="POST" action="/pokemon">
        <input hidden type="text" name="name" value="<%= pokemon.name %>">
        <button class="btn btn-primary" type="submit">Add to Favorite Pokemon</button>
      </form>
    </div>
  <% }); %>
</div>

