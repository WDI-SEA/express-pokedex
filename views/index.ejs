<div class="container">
  <h1 class="text-center">The Original 151 Pokemon</h1>
  <p class="text-center">
    <a href="/pokemon">Your Team</a>
  </p>

  <% pokemon.forEach(function(pokemon) { %>
    <div class="well">
      <h2><%= pokemon.name %></h2>
      <a href="/pokemon/new/<%=pokemon.name%>">
        <button class="btn btn-primary">Add to Your Team</button>
      </a>
    </div>
  <% }); %>
</div>
