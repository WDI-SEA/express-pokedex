<main>
  <% pokemon.forEach(function(pokemon) { %>
    <div class="well col-sm-4">
      <h2><%= pokemon.name %></h2>
      <form method="POST" action="/pokemon">
        <input hidden type="text" name="name" value="<%= pokemon.name %>">
        <button class="btn btn-primary add-favorite" type="submit">
          <img class="nav-icon" src="/img/pokeball.png" alt="favorites">
          Add to Favorites
        </button>
      </form>
    </div>
  <% }); %>
</main>