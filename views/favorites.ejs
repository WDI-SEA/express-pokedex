<div class="container">
<h1 class="text-center">Favorites</h1>
<p class="text-center">
    <a href="../">Back to Pokedex</a>
  </p>
<% pokemons.forEach(function(pokemon) { %>
	<div class="well">
	<h2><%= pokemon.name %></h2>
	<form action="/pokemon/<%=pokemon.id%>" method="GET">
		<button class="btn btn-primary" type="submit">More Deets</button>
	</form>
	</div>
<% }) %>
	</div>