<div class="container">
  <h1 class="text-center">Favorites</h1>
  <h2 class=><%= pokemon.name %></h2>

  <% pokemon.forEach(function(pokemon) { %>
    <div class="well">
      <h2><%= pokemon.name %></h2>
      <form class="delete-pokemon" action="/pokemon/<%= pokemon.id %>">
        <button class="btn btn-primary" type="submit">Delete</button>
      </form>
    </div>
  <% }); %>
</div>
