<div class="page-header">
  <h1 id="favorite" class="text-center">Favorite Pok√©mon</h1>
</div>
<div class="container">
  <% pokemon.forEach(function(pokemon) { %>
    <div class="well">
      <div class="pull-right">
        <span><i class="glyphicon glyphicon-flash"><a href="/pokemon/<%= pokemon.id %>" class="details"></i> 
          See Details
          </a>
          <hr>
          <i class="glyphicon glyphicon-remove"><a href="/pokemon/<%= pokemon.id %>" class="delete"></i>
           Remove
          </a></span>
      </div>
      <h2 id="favoritesName"><%= pokemon.name %></h2>
    </div>
  <% }); %>
</div>