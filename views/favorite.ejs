<div class="container">
    <h1 class="text-center">Favorites</h1>
    <p class="text-center">
      <a href="/">Home</a>
    </p>
  
    <% pokemon.forEach(function(pokemon) { %>
      <div class="well">
        <h2><%= pokemon.name %></h2>
        <form method="GET" action="/pokemon/<%= pokemon.name %>">
          <button class="btn btn-primary" type="submit">Details</button>
        </form>
        <br>
        <form method="POST" action="/pokemon/<%= pokemon.id %>/?_method=DELETE">
          <button class="btn btn-primary" type="submit" value="DELETE">Delete From Favorites</button>
        </form>
      </div>
    <% }); %>
  </div>