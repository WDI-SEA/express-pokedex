<h2><%=name%></h2>

<img src="<%=image%>" alt="picture of pokemon">

<h4>List of Abilities</h4>
<ul>
    <% abilities.forEach(ability => {%>
        <li><%=ability.ability.name%></li>
    })
</ul>

<h4>List of Moves</h4>
<ul>
    <% moves.forEach(move => {%>
        <li>
            <%=move.move.name%>
        </li>
    <%})%>
</ul>

<h4>List of Stats</h4>
<ul>
    <% stats.forEach(stat => {%>
        <li>
            <%=stats.stat.name%> = <%=stat.base_stat%>
        </li>
    <%})%>
</ul>
