<div id="pokedex">
	<a href="/" id="home">Home</a>
	<div id="infobox">
		<h3><%= result.name%></h3>
		<h4>Order: <%= result.order%></h4>
		<img src='<%=result.sprites.front_default%>'>
		<img src='<%=result.sprites.back_default%>'>
	</div>
	<div id="height">
		<h4><%= result.height%></h4>
	</div>
	<div id="weight">
		<h4><%= result.weight%></h4>
	</div>
	<div id="favorites">
		<h4><a href="/pokemon/">Favorites</a></h4>
	</div>
	<div id="ability">
		<h5>Ability: <%= result.abilities[0].ability.name%></h5>
	</div>
</h3>