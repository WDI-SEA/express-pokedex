<div class="container">
         <div class="well">
         <h1><%= pokemon.name %></h1>
         <img src="<%= pokemon.sprites.front_default %>" alt="<%= pokemon.name %>">
         <p>
          ID: <%= pokemon.id %></br>
          Order: <%= pokemon.order %>
          </p>
          <p>
          Height: <%= pokemon.height %> dm</br>
          Weight: <%= pokemon.weight %> hg
          </p>

          <p>
          Base Experience: <%= pokemon.base_experience %>
          </p>

          <p>
          Types: <%= pokemon.typesCommaSeperated %>
          </p>

        <a href="<%= "/" %>">
          Back to All Pokemon
        </a>
        </div>
</div>
